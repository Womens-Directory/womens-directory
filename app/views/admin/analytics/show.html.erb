<div class="autostyle">
  <%= link_to "Â« Back to all reports", :admin_analytics %>
  <h1>Analytics Report <%= @report.id %></h1>
  <ul>
    <li><strong>Generated:</strong> <%= @report.created_at %> (<%= time_ago_in_words(@report.created_at) %> ago)</li>
    <li><strong>Start Date:</strong> <%= @report.start_date %> (<%= time_ago_in_words(@report.start_date) %> ago)</li>
    <li><strong>End Date:</strong> <%= @report.end_date %> (<%= time_ago_in_words(@report.end_date) %> ago)</li>
    <li><strong>Duration:</strong> <%= distance_of_time_in_words(@report.start_date, @report.end_date) %></li>
  </ul>
  <% unless @report.data %>
    <p>This report is still being generated. Please check back later.</p>
  <% end %>
  <% if @report.data %>
    <%= @report.data %>
  <% end %>
</div>
